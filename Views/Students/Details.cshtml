@model Student
@{
    ViewBag.Title = "Details";
}
<h1>Details</h1>

<div>
    <h4>Student</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Last Name
        </dt>
        <dd class="col-sm-10">
            @Model.LastName
        </dd>
        <dt class="col-sm-2">
            First Name
        </dt>
        <dd class="col-sm-10">
            @Model.FirstName
        </dd>
        <dt class="col-sm-2">
            Enrollment Date
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(n => n.EnrollmentDate)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", "Students", new { id = Model.ID })
    @Html.Raw(" | ")
    @Html.ActionLink("Back to List","Index")
</div>
